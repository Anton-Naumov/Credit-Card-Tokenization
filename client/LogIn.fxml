<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="350.0" prefWidth="350.0" type="VBox" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" prefHeight="100.0" prefWidth="400.0" text="Log In Menu">
         <font>
            <Font name="Book Antiqua Bold" size="37.0" />
         </font>
         <VBox.margin>
            <Insets top="15.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="txtUsername" promptText="Username">
         <VBox.margin>
            <Insets left="70.0" right="70.0" top="25.0" />
         </VBox.margin>
      </TextField>
      <PasswordField fx:id="txtPassword" promptText="Password">
         <VBox.margin>
            <Insets left="70.0" right="70.0" top="25.0" />
         </VBox.margin>
      </PasswordField>
      <Button fx:id="btnLogIn" mnemonicParsing="false" onAction="#btnLogInClicked" prefWidth="170.0" text="Log In">
         <VBox.margin>
            <Insets left="90.0" right="90.0" top="25.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="btnQuit" mnemonicParsing="false" onAction="#btnQuitClicked" prefWidth="170.0" text="Quit">
         <VBox.margin>
            <Insets left="90.0" right="90.0" top="15.0" />
         </VBox.margin>
      </Button>
   </children>
</fx:root>
